		var otherProdOptIds = productOptionsIds.filter(id =&gt; id !== currentProdOptId);&#xd;
		var currentProdOpt = availableProductOptions.find(prodOpt =&gt; prodOpt.id === currentProdOptId);&#xd;
		var currentProdOptValues = currentProdOpt.optValues;&#xd;
		combinationMap[i] = {};&#xd;
